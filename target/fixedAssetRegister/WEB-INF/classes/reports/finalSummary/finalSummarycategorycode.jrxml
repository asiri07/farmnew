	<queryString><![CDATA[SELECT
  acm.MCAT_CODE,
  acs.MCAT_ID,
  acm.MCAT_DES,
  acs.SCAT_CODE,
  acs.SCAT_DES,
  acd.DCAT_CODE,
  acd.DCAT_DES,
  acd.DCAT_DES,
  BF_DISP_VALUE AS blcFord,
   SUM(ASSET_VALUE) AS addition,
  IFNULL(SUM(DISPOSAL_AMOUNT),0) AS disposal,
  SUM(TO_DATE_DISP_VAL) AS totdep,
  IFNULL(SUM(DISP_VALUE_DISPOSAL),0) AS Depval,
  RPD.START_DATE,
  RPD.END_DATE,
  RPD.CURRENT_VALUE AS WDE,RPD.REGISTED_DATE ,RPD.ASSET_VALUE,SUM(FROM_DATE_DISP_VAL) AS DepAtFdate,
  SUM(IF(RPD.REGISTED_DATE>=$P{FromDate},RPD.ASSET_VALUE,"0.0")) AS  additions,
    SUM(IF(RPD.REGISTED_DATE<$P{FromDate},RPD.ASSET_VALUE,"0.0")) AS  balanceBF
FROM report_preparation_data AS RPD,
  asset,
  asset_catergory_detail AS acd,
  asset_catergory_sub AS acs,
  asset_catergory_main AS acm,
  location_master AS lm,
  section_master AS sec,
  department_master AS dep 
WHERE asset.AS_CODE = RPD.ASSET_CODE
    AND acd.DCAT_ID = asset.AS_DCAT_ID
    AND acs.SCAT_ID = acd.SCAT_ID
    AND acm.MCAT_ID = acs.MCAT_ID
    AND asset.LOC_ID=lm.LOC_ID  
    AND lm.SEC_ID = sec.SEC_ID
    AND sec.DEP_ID = dep.DEP_ID 
AND acs.MCAT_ID BETWEEN $P{fromcord}
    AND $P{tocord}
$P!{query}
GROUP BY acs.MCAT_ID,acs.SCAT_ID]]></queryString>

